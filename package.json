{
  "name": "authify",
  "version": "1.0.0",
  "description": "A sample server initial setup in Node JS and with User and Authentication",
  "main": "server.js",
  "scripts": {
    "test": "jest",
    "start": "npm run lint && node server.js",
    "start:dev": "nodemon server.js",
    "lint": "npx eslint '**/*js'",
    "generate-signature": "node __tests__/generate-signature.js",
    "staging-bck": "pm2 start server.js --ignore-watch app/logs/error.log -i max --name kordie-api-staging",
    "staging:stop-bck": "pm2 stop kordie-api-staging && pm2 delete kordie-api-staging",
    "staging": "pm2 start process.json -i max --name kordie-api-process",
    "staging:stop": "pm2 stop kordie-api-staging && pm2 delete kordie-api-staging"
  },
  "keywords": [],
  "author": "Anik Das",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.676.0",
    "aws-sdk": "^2.1691.0",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "country-state-city": "^3.2.1",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.4.5",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-oauth2-jwt-bearer": "^1.6.0",
    "google-auth-library": "^9.15.0",
    "i18n": "^0.15.1",
    "jsonwebtoken": "^9.0.1",
    "moment": "^2.30.1",
    "mongoose": "^7.6.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "react-dropdown-select": "^4.11.4",
    "redis": "^4.6.10",
    "sib-api-v3-sdk": "^8.5.0",
    "stripe": "^17.5.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "twilio": "^5.3.6",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "eslint": "^8.48.0",
    "jest": "^29.6.3",
    "nodemon": "^3.0.1"
  }
}
